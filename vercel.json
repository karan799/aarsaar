{
    "version": 2,
    "builds": [
      { "src": "backend/server.js", "use": "@vercel/node" },
      { "src": "backend/frontend", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/backend/server.js" },
      { "src": "/(.*)", "dest": "/backend/frontend/index.html" }
    ]
  }
  